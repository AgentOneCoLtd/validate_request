  "peerDependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/joi": "^15.0.3",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@hapi/boom": "^7.4.2",
    "@hapi/joi": "^15.0.3",
    "@types/express": "^4.17.0",
    "@types/hapi__boom": "^7.4.0",
    "@types/hapi__joi": "^15.0.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "express": "^4.17.1",
    "jest": "^24.8.0",
    "joi": "^14.3.1",
